{
  "name": "angular2-express-starter",
  "version": "1.0.0",
  "description": "Starter application of Angular2 with Express",
  "main": "server/app.js",
  "license": "MIT",
  "scripts": {
    "start": "echo **** Starting for Production **** && npm run build && node dist/server/bin/www.js",
    "start_dev": "echo **** Starting for DEV **** && concurrently --kill-others \"npm run _server:run\" \"ng serve --aot --progress=false --proxy-config proxy.conf.json\"",
    "lint:client": "ng lint",
    "lint:server": "tslint './server/**/*.ts' -c ./server/tslint.json --fix",
    "test:client": "ng test",
    "e2e:client": "ng e2e",
    "build": "ng build --prod --sm=false --aot --output-path=dist/client && npm run _server:build",
    "_server:run": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"nodemon --delay 5 dist/server/bin/www.js\" ",
    "_server:build": "tsc -p ./server",
    "postinstall": "npm run build"
  },
  "author": "Vlado Tešanović",
  "repository": {
    "url": "https://github.com/vladotesanovic/angular2-express-starter"
  },
  "private": true,
  "dependencies": {
